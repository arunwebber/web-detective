{
    "manifest_version": 3,
    "name": "Web Detective",
    "version": "1.1.1",
    "description": "Analyze a webpage for SEO, security, performance, and more.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Web Detective",
        "default_icon": {
            "16": "images/icon_16.png",
            "48": "images/icon_48.png",
            "128": "images/icon_128.png"
         }
    },
    "icons": {
      "16": "images/icon_16.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "homepage_url": "https://www.arunsyoga.in", 
    "author": "HashPalLabs"
  
}
